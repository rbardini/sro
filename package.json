{
  "name": "sro",
  "version": "0.1.6",
  "description": "Friendly Correios SRO API wrapper and command-line utility",
  "author": "Rafael Bardini",
  "homepage": "https://github.com/rbardini/sro",
  "keywords": [
    "correios",
    "ect",
    "sro",
    "postal",
    "mail",
    "package",
    "tracking",
    "shipping",
    "brazil"
  ],
  "main": "index.js",
  "scripts": {
    "build": "babel src/ -d lib/",
    "watch": "npm run build -- --watch",
    "lint": "snazzy",
    "pretestone": "npm run build && npm run lint",
    "testone": "tap --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill --coverage --coverage-report=text-summary",
    "test": "npm run testone test/*.js",
    "prepublish": "npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": "./bin/sro",
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/sro"
  },
  "bugs": {
    "url": "https://github.com/rbardini/sro/issues"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "country-data": "0.0.24",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "request": "^2.72.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "nyc": "^6.4.4",
    "sinon": "^1.17.4",
    "snazzy": "^4.0.0",
    "standard": "^7.1.0",
    "tap": "^5.7.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
