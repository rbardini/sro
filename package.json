    "commander": "^3.0.1",
    "country-data": "^0.0.31",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "request": "^2.72.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^10.0.3",
