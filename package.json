{
  "name": "sro",
  "version": "0.1.6",
  "description": "Friendly Correios SRO API wrapper and command-line utility",
  "author": "Rafael Bardini",
  "homepage": "https://github.com/rbardini/sro",
  "keywords": [
    "correios",
    "ect",
    "sro",
    "postal",
    "mail",
    "package",
    "tracking",
    "shipping",
    "brazil"
  ],
  "main": "index.js",
  "scripts": {
    "build": "babel src/ -d lib/",
    "watch": "npm run build -- --watch",
    "lint": "standard | snazzy",
    "pretest": "npm run build && npm run lint",
    "test": "tap test/*.js",
    "prepublish": "npm test",
    "coverage": "tap test/*.js --coverage-report=text-lcov | coveralls"
  },
  "bin": "./bin/sro",
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/sro"
  },
  "bugs": {
    "url": "https://github.com/rbardini/sro/issues"
  },
  "license": "MIT",
  "dependencies": {
    "async": "3.1.0",
    "chalk": "3.0.0",
    "cli-table": "0.3.1",
    "commander": "4.0.1",
    "country-data": "0.0.31",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "request": "2.88.0",
    "xml2js": "0.4.22"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "babel-plugin-add-module-exports": "1.0.2",
    "coveralls": "3.0.9",
    "sinon": "7.5.0",
    "snazzy": "8.0.0",
    "standard": "14.3.1",
    "tap": "14.10.2"
  }
}
