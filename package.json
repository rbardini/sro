    "url": "https://github.com/rbardini/sro/raw/master/LICENSE"
  },
  "dependencies": {
    "async": "^1.4.2",
    "chalk": "^1.1.1",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "country-data": "0.0.23",
    "iconv-lite": "^0.4.13",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "request": "^2.65.0",
    "xml2js": "^0.4.13"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "chai-things": "^0.2.0",
    "coveralls": "^2.11.4",
