{
  "name": "sro",
  "version": "0.1.5",
  "description": "Friendly Correios SRO API wrapper and command-line utility",
  "author": "Rafael Bardini",
  "homepage": "https://github.com/rbardini/sro",
  "keywords": [
    "correios",
    "ect",
    "sro",
    "postal",
    "mail",
    "package",
    "tracking",
    "shipping",
    "brazil"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "snazzy",
    "pretest": "npm run build && npm run lint",
    "test": "mocha",
    "build": "babel src/ -d lib/",
    "watch": "npm run build -- --watch",
    "prepublish": "npm test",
    "precoverage": "npm run build",
    "coverage": "istanbul cover _mocha",
    "preci": "npm run pretest",
    "ci": "istanbul cover _mocha --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "postci": "rm -rf ./coverage"
  },
  "bin": "./bin/sro",
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/sro"
  },
  "bugs": {
    "url": "https://github.com/rbardini/sro/issues"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "country-data": "0.0.24",
    "iconv-lite": "^0.4.13",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "request": "^2.72.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-things": "^0.2.0",
    "coveralls": "^2.11.9",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.4",
    "snazzy": "^4.0.0",
    "standard": "^7.1.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
