{
  "name": "sro",
  "version": "0.1.5",
  "description": "Friendly Correios SRO API wrapper and command-line utility",
  "author": "Rafael Bardini",
  "homepage": "https://github.com/rbardini/sro",
  "keywords": [
    "correios",
    "ect",
    "sro",
    "postal",
    "mail",
    "package",
    "tracking",
    "shipping",
    "brazil"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "snazzy",
    "pretest": "npm run lint",
